						</input.group>
					</col>
				</form.group>
				<form.group class="has-error">
					<label class="col-sm-2 control-label">Password</label>
					<col sm="10">
						<input.group>
							<input.group.addon>$</input.group.addon>
							<input.form.control type="password" placeholder="Password" size="sm" />
						</input.group>
					</col>
				</form.group>
