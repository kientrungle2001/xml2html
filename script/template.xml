			<form.horizontal>
				<form.group class="has-warning">
					<label class="col-sm-2 control-label">Name</label>
					<col sm="10">
						<static.form.control>Jane Doe</static.form.control>
					</col>
				</form.group>
				<form.group class="has-success has-feedback">
					<label class="col-sm-2 control-label">Email address</label>
					<col sm="10">
						<input.group>
							<input.group.addon>@</input.group.addon>
							<input.form.control type="email" placeholder="Email" size="lg" />
