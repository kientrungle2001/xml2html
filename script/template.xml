				</form.group>
				<div class="checkbox">
					<label>
						<checkbox /> Check me out
					</label>
				</div>
				<btn.default>Submit</btn.default>
			</form>
			<form.inline>
				<form.group>
					<label>Name</label>
					<input.form.control placeholder="Jane Doe" />
				</form.group>
				<form.group>
					<label>Email</label>
					<input.form.control type="email" placeholder="jane.doe@example.com" />
				</form.group>
				<btn.default type="submit">Send Invitation</btn.default>
